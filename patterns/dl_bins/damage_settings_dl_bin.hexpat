struct DamageStatusEffectInfo {
    StatusEffectType type;
    float value;
};

struct DamageInfo {
    DamageInfoType type;
    s32 damage[2];
    u32 armor_penetration_per_angle[4];
    u32 demolition_strength;
    u32 force_strength;
    u32 force_impulse;
    ElementType element_type;
    DamageStatusEffectInfo status_effects[4];
};

struct DamageSettings {
    u64 offset;
    u64 count;
    if (offset != U64_MAX) {
        DamageInfo damage_infos[count] @ addressof(this) + offset;
    }
};

struct DLItem {
    char magic[4];
    u32 version;
    DLItemType type;
    u32 size;
    u32 unk02;
    u32 unk03;
    match(type) {
        (DLItemType::DamageSettings): DamageSettings data;
        (_): u8 data[size];
    }
    padding[size-sizeof(data)];
};

struct DLBin {
    u32 count;
    DLItem items[count];
};

DLBin dlbin @0x00;